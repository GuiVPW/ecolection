{
  "name": "ecolection",
  "version": "1.0.0",
  "main": "src/app/main/index.ts",
  "license": "MIT",
  "scripts": {
    "dev": "env-cmd ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/app/main/server.ts",
    "start": "env-cmd node dist/server.js",
    "test": "env-cmd jest --passWithNoTests --silent --runInBand --detectOpenHandles",
    "test:verbose": "env-cmd jest --passWithNoTests --runInBand",
    "test:unit": "env-cmd yarn test --watch -c jest-unit.config.ts",
    "test:integration": "env-cmd yarn test --watch -c jest-integration.config.ts",
    "test:staged": "env-cmd yarn test --findRelatedTests",
    "test:ci": "env-cmd yarn test --coverage",
    "migrate:generate": "prisma migrate dev --preview-feature --name",
    "migrate:reset": "prisma migrate reset --preview-feature",
    "database:generate": "prisma generate",
    "run:seeds": "prisma db seed --preview-feature",
    "studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^2.17.0",
    "apollo-server": "^2.21.0",
    "apollo-server-express": "^2.21.0",
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-fields": "^2.0.3",
    "graphql-ws": "^4.1.6",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "prisma": "^2.17.0"
  },
  "devDependencies": {
    "@prisma/cli": "^2.17.0",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.11",
    "@types/graphql-depth-limit": "^1.1.2",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/method-override": "^0.0.31",
    "@types/module-alias": "^2.0.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.31",
    "@types/node-emoji": "^1.8.1",
    "@types/supertest": "^2.0.10",
    "env-cmd": "^10.1.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "git-commit-msg-linter": "^3.0.0",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "module-alias": "^2.2.2",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.5"
  },
  "_moduleAliases": {
    "@main": "./src/app/main",
    "@presentations": "./src/app/presentations",
    "@domain": "./src/app/domain",
    "@infra": "./src/app/infra",
    "@data": "./src/app/data"
  }
}
